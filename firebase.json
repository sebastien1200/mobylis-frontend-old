{
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "/post/test",
        "function": "helloWorld"
      },
      {
        "source": "/api/*",
        "destination": "http://localhost:8080/test",
        "type": 200
      }
    ]
  },
  "functions": {
    "predeploy": "npm --prefix functions run build"
  }
}
