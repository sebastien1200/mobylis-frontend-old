{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/post/test",
        "function": "helloWorld"
      }
    ],
    "redirects": [ {
      "source" : "/api/test",
      "destination" : "http://localhost:8080/test",
      "type" : 301
    } ]
  },
  "functions": {
    "predeploy": "npm --prefix functions run build"
  }
}
